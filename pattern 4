import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(i<n-1 && j<n-i-1){
                    System.out.print(1+" ");
                }else{
                    System.out.print((i+1)+" ");
                }
            }
            System.out.println();
        }
    }
}
